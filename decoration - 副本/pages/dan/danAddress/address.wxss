/* pages/dan/danAddress/address.wxss */
.address_box{
  padding:10rpx 20rpx 0 20rpx;
  background: #fff;
}
.content_caozuo{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  border-bottom:2rpx solid #eaeaea;
  height:80rpx;
  line-height:80rpx;
  color:#92DCE5;
}
.content .content_caozuo:first-child{
  color:#8E8E8E;
}
.xiugai{
  margin-right:20rpx;
  color:#8E8E8E;
}
.shanchu{
  color:#8E8E8E;
}
.user_info{
  height:70rpx;
  line-height:70rpx;
  border-bottom:2rpx dashed #eaeaea;
}
.flod{
  height:70rpx;
  line-height:70rpx;
  border-bottom:2rpx solid #eaeaea;
  overflow: hidden;
}
.extend{
  height:140rpx;
  border-bottom:2rpx solid #eaeaea;
}
.extend>text{
  height:70rpx;
  line-height:50rpx;
}