body{
  font-family: Arial,'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans',  sans-serif;
}
.classify_text {
  font-weight: normal;
  font-style: normal;
}

/*搜索框*/

.search {
  height: 33px;
  width: 100%;
  padding-top: 6px;
  padding-bottom: 5px;
  background: -webkit-gradient(linear, 0% 0%, 100% 0%, from(#73d7e9), to(#5adccb));
  overflow: hidden;
}

.search_input {
  width: 90%;
  height: 28px;
  border: 1px solid #ddd;
  border-radius: 5px;
  background: #fff;
  margin: 0 auto;
  /*position:relative;*/
  text-align: center;
}

.search_icon {
  position: relative;
  top: -25px;
  left: 25px;
  z-index: 10;
  text-align: center;
  overflow: hidden;
}
/*轮播*/

.slider-image {
  width: 100%;
  height:150px;
}

/*分类*/

.classify_box {
  height: 160px;
  background: #fff;
  padding: 15px 20px;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.classify {
  text-align: center;
  width: 25%;
}

.classify_pic {
  width: 40px;
  height: 40px;
}

.classify_text {
  font-size: 12px;
  display: block;
  margin-top: 5px;
}

/*装修案例*/

.case_box {
  margin-top: 10px;
  margin-bottom: 10px;
  background: #fff;
}

.case {
  padding: 15px 0 10px 15px;
  border-bottom: 1px solid #eee;
}

.case:last-child {
  border-bottom: 0;
}

.case_title {
  margin-right: 15px;
  margin-bottom: 15px;
  display: flex;
  justify-content: space-between;
}

.case_title>text {
  font-size: 16px;
  font-weight: 700;
}

.case_title>navigator {
  font-size: 12px;
  font-weight: 700;
  color: #919191;
}

.case_scroll {
  padding-right: 10px;
  margin-bottom: 15px;
}

.slider_image {
  width: 100%;
  height: 125px;
  display: block;
  margin-bottom: 10px;
}

.user_pic {
  width: 16px;
  height: 16px;
  margin-right: 5px;
  vertical-align:middle;
}

.case_scroll .text {
  font-size: 14px;
  font-weight: 700;
  display: block;
  margin-bottom: 5px;
}

.case_scroll .text_small {
  font-size: 12px;
  font-weight: 500;
  color: #888;
  display: block;
  line-height: 13px;
}

